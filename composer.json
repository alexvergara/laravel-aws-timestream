{
    "name": "ringierimu/laravel-aws-timestream",
    "description": "Library to interact with AWS Timestream service via API",
    "type": "library",
    "extra": {
        "laravel": {
            "providers": [
                "Ringierimu\\AwsTimestream\\TimestreamServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Ringierimu\\AwsTimestream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ringierimu\\AwsTimestream\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Norby Baruani",
            "email": "norbyb@ringier.co.za"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "aws/aws-sdk-php": "^3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.24"
    }
}
