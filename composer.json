{
  "name": "alexvergara/laravel-aws-timestream",
  "description": "Fork from Norbybaru - Library to interact with AWS Timestream service via API - Downgrade for Laravel6 + PHP7 - Fixes for AWS Config",
  "keywords": [
    "aws timestream",
    "laravel aws timestream",
    "laravel timeseries db",
    "timeseries db"
  ],
  "type": "library",
  "homepage": "https://github.com/norbybaru/laravel-aws-timestream",
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "AlexVergara\\AwsTimestream\\TimestreamServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "AlexVergara\\AwsTimestream\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlexVergara\\AwsTimestream\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Alex Vergara",
      "email": "alex.vergara@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "5.6.*",
    "aws/aws-sdk-php": "^3.209"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.34"
    },
    "allow-plugins": {
      "kylekatarnls/update-helper": false
    }
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.5",
    "vlucas/phpdotenv": "^5.4"
  }
}
